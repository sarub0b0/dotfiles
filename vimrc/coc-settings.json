{
  "clangd.arguments": [
    "--fallback-style=google"
  ],
  "clangd.fallbackFlags": [
    "-I",
    "include/"
  ],
  "coc-actions.useCursorLine": false,
  "coc-actions.showActionKind": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "*"
  ],
  "coc.preferences.useQuickfixForLocations": false,
  "coc.source.emoji.filetypes": [
    "markdown",
    "vue",
    "javascript"
  ],
  "diagnostic.errorSign": "\uf00d",
  "diagnostic.hintSign": ">>",
  "diagnostic.infoSign": ">>",
  "diagnostic.warningSign": "\uf12a",
  "eslint.autoFixOnSave": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "u": "gotoParent"
  },
  "explorer.previewAction.onHover": false,
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": true
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.width": 25,
  "languageserver": {
    "cmake": {
      "command": "cmake-language-server",
      "filetypes": [
        "cmake"
      ],
      "rootPatterns": [
        "build/"
      ],
      "initializationOptions": {
        "buildDirectory": "build"
      }
    }
  },
  "prettier.disableLanguages": [
    "vue",
    "json"
  ],
  "prettier.trailingComma": "all",
  "python.jediEnabled": false,
  "rust-analyzer.diagnostics.disabled": [
    "macro-error",
    "inactive-code"
  ],
  "rust-analyzer.updates.channel": "stable",
  "sh.enable": true,
  "solargraph.formatting": true,
  "snippets.userSnippetsDirectory": "~/.vim/snippets/",
  "snippets.snipmate.enable": false,
  "translator.toLang": "ja",
  "translator.engines": [
    "google"
  ],
  "tsserver.trace.server": "verbose",
  "yaml.schemas": {
    "kubernetes": [
      "manifest*/**/*.yaml",
      "/*.yaml"
    ],
    "https://json.schemastore.org/gitlab-ci": [
      "/**/.gitlab-ci.yml",
      "/**/.gitlab/*.y*ml"
    ]
  },
  "yaml.schemaStore.enable": true,
  "cSpell.userWords": [
    "Appender",
    "Autoclose",
    "Insertable",
    "Kube",
    "Kubeconfig",
    "Nerdfont",
    "Quickfix",
    "SAST",
    "Termion",
    "Varchar",
    "actix",
    "apimachinery",
    "appprojects",
    "aquasec",
    "argocd",
    "argoproj",
    "auths",
    "autodiscover",
    "automerge",
    "autoresize",
    "backported",
    "bbtower",
    "bbtservice",
    "chrono",
    "clangd",
    "clusterrole",
    "clusterrolebinding",
    "cmake",
    "codeclimate",
    "codequality",
    "concat",
    "configmap",
    "configmaps",
    "crossterm",
    "datetime",
    "devteam",
    "dotenv",
    "dprintln",
    "entrypoint",
    "focusable",
    "fullwidth",
    "gcloud",
    "gserviceaccount",
    "gsutil",
    "healthz",
    "himawari",
    "hostnames",
    "inplace",
    "kaniko",
    "kubectl",
    "kubernetes",
    "kubetui",
    "letsencrypt",
    "logfile",
    "microservices",
    "mikefarah",
    "mkdir",
    "netrc",
    "openapi",
    "prettierrc",
    "printf",
    "renovatebot",
    "rpod",
    "sarub",
    "sarub0b0",
    "serviceaccount",
    "snipmate",
    "solargraph",
    "subwin",
    "thiserror",
    "trivy",
    "trivycache",
    "whitesource",
    "workdir",
    "xargs"
  ]
}
