{
  "clangd.arguments": ["--fallback-style=google"],
  "clangd.fallbackFlags": ["-I", "include/"],
  "coc-actions.useCursorLine": false,
  "coc-actions.showActionKind": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.useQuickfixForLocations": false,
  "coc.source.emoji.filetypes": ["markdown", "vue", "javascript"],
  "diagnostic.errorSign": "\uf00d",
  "diagnostic.hintSign": ">>",
  "diagnostic.infoSign": ">>",
  "diagnostic.warningSign": "\uf12a",
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "eslint.autoFixOnSave": true,
  "explorer.file.autoReveal": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.enableVimDevicons": false,
  "explorer.keyMappings.global": {
    "u": "gotoParent"
  },
  "explorer.previewAction.onHover": false,
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": true
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.width": 25,
  "languageserver": {
    "cmake": {
      "command": "cmake-language-server",
      "filetypes": ["cmake"],
      "rootPatterns": ["build/"],
      "initializationOptions": {
        "buildDirectory": "build"
      }
    }
  },
  "python.jediEnabled": false,
  "sh.enable": true,
  "solargraph.formatting": true,
  "translator.toLang": "ja",
  "translator.engines": ["google"],
  "tsserver.trace.server": "verbose",
  "yaml.schemas": {
    "https://json.schemastore.org/composer": "/*",
    "kubernetes": "/*.yaml",
    "https://json.schemastore.org/gitlab-ci": [
      ".gitlab-ci.yml",
      ".gitlab/*.yml"
    ]
  }
}
